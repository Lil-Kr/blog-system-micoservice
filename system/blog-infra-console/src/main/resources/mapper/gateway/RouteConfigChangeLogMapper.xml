<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cy.micoservice.blog.infra.console.dao.gateway.RouteChangeLogMapper">

	<sql id="Table_Name">
		t_route_change_log
	</sql>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		id, config_id, version, change_body, change_event, create_by, create_time, deleted
	</sql>
	
	<select id="findGtVersion" resultType="org.cy.micoservice.blog.entity.gateway.model.entity.RouteChangeLog">
		select <include refid="Table_Name" />
		from <include refid="Base_Column_List" />
		<where>
			version > #{version}
		</where>
	</select>

</mapper>
