<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.cy.micoservice.blog</groupId>
		<artifactId>blog-im</artifactId>
		<version>1.0.0</version>
	</parent>
	
	<artifactId>blog-im-router</artifactId>
	<packaging>jar</packaging>
	
	<properties>
		<maven.compiler.source>17</maven.compiler.source>
		<maven.compiler.target>17</maven.compiler.target>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<blog-common.version>1.0.0</blog-common.version>
		<blog-entity-parent.version>1.0.0</blog-entity-parent.version>
		<blog-framework-parent.version>1.0.0</blog-framework-parent.version>
		<blog-im-facade.version>1.0.0</blog-im-facade.version>
	</properties>
	
	<dependencies>
		<dependency>
			<groupId>com.alibaba.cloud</groupId>
			<artifactId>spring-cloud-starter-alibaba-nacos-discovery</artifactId>
		</dependency>
		
		<dependency>
			<groupId>com.alibaba.cloud</groupId>
			<artifactId>spring-cloud-starter-alibaba-nacos-config</artifactId>
		</dependency>
		
		<dependency>
			<groupId>org.springframework.cloud</groupId>
			<artifactId>spring-cloud-starter-bootstrap</artifactId>
		</dependency>
		
		<!-- test start -->
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<scope>test</scope>
		</dependency>
		
		<dependency>
			<groupId>org.junit.jupiter</groupId>
			<artifactId>junit-jupiter-api</artifactId>
		</dependency>
		
		<dependency>
			<groupId>org.junit.jupiter</groupId>
			<artifactId>junit-jupiter-engine</artifactId>
		</dependency>
		<!-- test end -->
		
		<!--自定义组件 -->
		<dependency>
			<groupId>org.cy.micoservice.blog</groupId>
			<artifactId>blog-common</artifactId>
			<version>${blog-common.version}</version>
		</dependency>
		
		<dependency>
			<groupId>org.cy.micoservice.blog</groupId>
			<artifactId>blog-framework-identify-starter</artifactId>
			<version>${blog-framework-parent.version}</version>
		</dependency>
		
		<dependency>
			<groupId>org.cy.micoservice.blog</groupId>
			<artifactId>blog-framework-rocketmq-starter</artifactId>
			<version>${blog-framework-parent.version}</version>
		</dependency>
		
		<dependency>
			<groupId>org.cy.micoservice.blog</groupId>
			<artifactId>blog-framework-cache-starter</artifactId>
			<version>${blog-framework-parent.version}</version>
			<exclusions>
				<exclusion>
					<artifactId>fastjson2</artifactId>
					<groupId>com.alibaba.fastjson2</groupId>
				</exclusion>
			</exclusions>
		</dependency>
		
		<dependency>
			<groupId>org.cy.micoservice.blog</groupId>
			<artifactId>blog-framework-dubbo-starter</artifactId>
			<version>${blog-framework-parent.version}</version>
		</dependency>
		
		<dependency>
			<groupId>org.cy.micoservice.blog</groupId>
			<artifactId>blog-im-facade</artifactId>
			<version>${blog-im-facade.version}</version>
		</dependency>
	</dependencies>
</project>
